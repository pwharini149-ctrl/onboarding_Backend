<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd">
   
<!-- Include global beans declaration file -->
	<import resource="../beans.xml" />
	<alias alias="itemDataRetrieverServiceLocal" name="cachedRCatItemDataRetrieverService" />
	<bean id="productPageIdentifier"
		class="amazon.cmt.afm.dpidentifier.ContentBasedProductPageIdentifier">
	</bean>
	<bean id="dataRecordExtractor" class="amazon.cmt.dre.XPathBasedDataRecordExtraction">
	</bean>
	<bean id="linkExtractor" class="amazon.cmt.dre.linkextractor.ContentBasedLinkExtractor"> 
	</bean> 
	<util:list id="scraperConfigs" list-class="java.util.ArrayList">
		<ref bean="xpathScraperConfig" />
		<ref bean="regexScraperConfig" />
	</util:list>
	
</beans>