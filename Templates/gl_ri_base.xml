<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd">
	<import resource="../beans.xml" />
	<util:list id="asinDataEnrichers" list-class="java.util.ArrayList">
		<ref bean="xmlCharacterCleaner" />
		<ref bean="mixedCaseAttributeNamedTransformer" />
		<ref bean="lowerCaseAttributeValueTransformer" />
		<ref bean="sourceAttributeValueTokenizer" />
	</util:list>
	<util:list id="competitorDataEnrichers" list-class="java.util.ArrayList">
		<ref bean="xmlCharacterCleaner" />
		<ref bean="mixedCaseAttributeNamedTransformer" />
		<ref bean="lowerCaseAttributeValueTransformer" />
		<ref bean="targetAttributeValueTokenizer" />
	</util:list>
    <!-- UTIL_LIST_BLOCK -->
    <!-- GENERATOR_BLOCK -->
</beans>
